{
  "name": "msb-auth-express",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "sequelize-cli db:migrate --config sequelize.cjs",
